{
  "name": "VoWi TISS Integration",
  "version": "0.1.0",
  "description": "Adds links to VoWi entries inside TISS. *Note: Not affiliated with TU Wien or FSInf",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": ["https://tiss.tuwien.ac.at/education/favorites.xhtml*"],
      "js": ["common.js", "favorites.js"]
    },
    {
      "matches": ["https://tiss.tuwien.ac.at/course/courseDetails.xhtml*", "https://tiss.tuwien.ac.at/course/educationDetails.xhtml*"],
      "js": ["common.js", "courseDetails.js"]
    }

  ]
}